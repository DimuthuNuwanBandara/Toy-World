<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/flower_back"
    tools:context=".RegStudents">

    <LinearLayout
        android:layout_width="309dp"
        android:layout_height="516dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="107dp">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter User ID" />

        <ProgressBar
            android:id="@+id/productprogressBar13"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/usersID"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:ems="10"
            android:hint="Enter User Id"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter User Full Name" />

        <ProgressBar
            android:id="@+id/productprogressBar4"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/f_name"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:ems="10"
            android:hint="Enter User Full Name"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter User Age" />

        <ProgressBar
            android:id="@+id/productprogressBar6"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/age"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:ems="10"
            android:hint="Enter User Age"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter Date Of Birth" />

        <ProgressBar
            android:id="@+id/productprogressBar7"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/dob"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:ems="10"
            android:hint="Enter Date Of Birth"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter Address" />

        <ProgressBar
            android:id="@+id/productprogressBar9"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/address"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:ems="10"
            android:hint="Enter Address"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Enter Contact Number" />

        <ProgressBar
            android:id="@+id/productprogressBar10"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/contact_no"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:ems="10"
            android:hint="Enter Contact No"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter Email" />

        <ProgressBar
            android:id="@+id/productprogressBar11"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/contact_no2"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:ems="10"
            android:hint="Enter Contact No"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="121dp"
            android:layout_height="wrap_content"
            android:text="Enter Password" />

        <ProgressBar
            android:id="@+id/productprogressBar14"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#51676060" />

        <EditText
            android:id="@+id/contact_no4"
            android:layout_width="match_parent"
            android:layout_height="21dp"
            android:ems="10"
            android:hint="Enter Contact No"
            android:inputType="textPersonName" />

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#9C27B0"
            android:text="Register"
            android:textColor="@android:color/background_light" />

    </LinearLayout>

    <TextView
        android:id="@+id/tvRegusers"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:text="Register Users"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="37dp" />
</androidx.constraintlayout.widget.ConstraintLayout>